			"__args": ["serve", "-debug", "0.0.0.0:8484", "-rpc.trace"],
			"rootPatterns": ["go.mod", "go.work", ".git/"],
			"__trace.server": "verbose",
			"filetypes": ["go"]
		},
		"bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh"],
			"__ignoredRootPaths": ["~"]
